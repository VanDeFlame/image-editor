{
	"plugins": ["prettier-plugin-tailwindcss"],
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"jsxSingleQuote": true,
	"quoteProps": "as-needed",
	"proseWrap": "preserve",
	"printWidth": 80,
	"bracketSameLine": false,
	"endOfLine": "lf",
	"singleAttributePerLine": false,
	"arrowParens": "always",
	"htmlWhitespaceSensitivity": "css"
}
